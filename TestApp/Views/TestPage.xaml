<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
   xmlns:mvvmaspire="clr-namespace:MvvmAspire.Controls;assembly=MvvmAspire"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="TestApp.Views.TestPage">
    <Grid>


        <StackLayout VerticalOptions="Start" Margin="20,50,20,0" HorizontalOptions="FillAndExpand">
            <mvvmaspire:RoundedButton Text="{Binding ButtonName}" BorderColor="Blue" TextColor="Blue" BorderWidth="1" Command="{Binding GoToNextPageCommand}"/>
            <mvvmaspire:RoundedButton Text="Sample Repository"  BorderColor="Green" TextColor="Green" BorderWidth="1" Command="{Binding GoToTestRepositoryCommand}"/>  
        </StackLayout>
   
        
    </Grid>
</ContentPage>
